python -O NoClergy/Python/make_ly.py $@ &gt; lilypond/ly/$@.ly
}

renderLy() {
lilypond --png -o lilypond/out/ lilypond/ly/$@.ly &gt;&gt;stdout.txt 2&gt;&gt;stderr.txt
}
