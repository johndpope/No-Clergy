import random

class Markov:
  """
  A set of operations dealing with Markov chains,
  organized into a single Object.
  """

  def __init__(self):
  """
  Create empty dictionaries for each note attribute.
  """
  self.pcD = {}
  self.midi_pitchD = {}
  self.artD = {}
  self.dynD = {}
  self.durD = {}

  def check(self, attr):
  """
  Double-check contents of the Markov Chain.
  """
  if attr == 'dur':
  dict = self.durD
  if attr == 'dyn':
  dict = self.dynD
  elif attr == 'art':
  dict = self.artD
  elif attr == 'midi_pitch':
  dict = self.midi_pitchD
  itemL = dict.items()
  # rewrite to accomodate 2nd order chain
  for item in itemL:
  (key, values) = item
  print key, '=&gt;',
  for value in values:
